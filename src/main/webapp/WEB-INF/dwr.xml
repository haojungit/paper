<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC "-//GetAhead Limited//DTD Direct Web Remoting 2.0//EN" "http://getahead.org/dwr/dwr20.dtd">
<dwr>

	<allow>
		<!-- create 中 javascript 为要生成的js名称和前台调用的主方法 create 因为使用SSH 框架,由spring代理,所以设置成spring -->

		<create creator="spring" javascript="userDWR">
			<!-- param中name因为要调用bean中的方法,所以使用beanName,value是spring中的bean -->
			<param name="beanName" value="paperUserAction" />
		</create>
